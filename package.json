{
  "name": "news-outlet",
  "description": "A new outlet style theme for Ghost",
  "demo": "",
  "version": "1.0.0",
  "engines": {
    "ghost": ">=2.0.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "gscan ."
  },
  "repository": {
    "type": "git",
    "url": "MIT"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Newspaper",
      "Broadsheet",
      "Magazine"
    ]
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme",
    "blog",
    "magazine",
    "newspaper"
  ],
  "author": {
    "name": "Fried-Chicken",
    "email": "Fried-Chicken@users.noreply.github.com",
    "url": "https://github.com/Fried-Chicken"
  },
  "bugs": "https://github.com/Fried-Chicken/ghost-theme-news-outlet/issues",
  "contributors": "https://github.com/Fried-Chicken/ghost-theme-news-outlet/graphs/contributors",
  "license": "MIT",
  "screenshots": {
    "desktop": "assets/screenshot-desktop.jpg",
    "mobile": "assets/screenshot-mobile.jpg"
  },
  "devDependencies": {
    "gscan": "^2.0.0"
  },
  "config": {
    "posts_per_page": 25,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
